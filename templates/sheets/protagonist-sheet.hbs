<form class="metanthropes {{cssClass}}" autocomplete="off">
	<header>
		<div class="layout-container-grid3x1 style-container-grid3x1">
			<div class="layout-container-small style-container-small">
				<div>Player
					<input
						name="system.metaowner"
						type="text"
						value="{{system.metaowner.value}}"
						placeholder="Player Name"
						data-dtype="string"
					/></div>
				<img src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="100" width="100" />
				<div><input name="name" type="text" value="{{actor.name}}" placeholder="A Protagonist" /></div>
			</div>
			<div class="layout-container-small style-container-small">
				<div>Prime Metapower <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<div>Metamorphosis <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<div>Background <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<div>Arc <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<p>Regression <input name="" type="text" value="" data-dtype="" placeholder="" /></p>
			</div>
			<div class="layout-container-small style-container-small">
				<div>Birthplace <input name="" type="text" value="" data-dtype="string" placeholder="" /></div>
				<div>Age
					<input
						name="system.vital.age.value"
						type="number"
						value="{{system.vital.age.value}}"
						data-dtype="number"
						placeholder=""
					/></div>
				<div>Gender <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<div>Height <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
				<div>Weight <input name="" type="text" value="" data-dtype="" placeholder="" /></div>
			</div>
		</div>
		<div class="layout-container-grid1x3 style-container-grid1x3">
			<div class="layout-container-small style-container-small">
				<div>
					Life ❤️
					<input
						name="system.vital.life.value"
						type="number"
						value="{{system.vital.life.value}}"
						data-dtype="number"
					/>
					<label
						for="system.vital.life.max"
						data-label="{{system.vital.life.max}}"
						class="style-labels-number"
					>{{system.vital.life.max}}</label>
				</div>
			</div>
			<div class="layout-container-small style-container-small">
				<div>Experience <input name="" type="number" value="" data-dtype="" placeholder="" /></div>
			</div>
			<div class="layout-container-small style-container-small">
				<div>Destiny 🤞
					<input
						name="system.vital.destiny.value"
						type="number"
						value="{{system.vital.destiny.value}}"
						data-dtype="number"
						placeholder="0"
					/></div>
			</div>
		</div>
	</header>
