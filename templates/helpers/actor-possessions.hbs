<ol class="items-list">
	{{#each Possessions as |Possessions Category|}}
		<li class="item flexrow items-header">
			<div class="item-name">{{Category}}</div>
			<div class="item-statroll">ðŸŽ²</div>
			<div class="item-controls"><i class="fa-thin fa-gears"></i> </div>
		</li>
		{{#each Possessions as |item id|}}
			<li class="item flexrow" data-item-id="{{item._id}}">
				<div class="item-name">
					<div class="item-image">
						<a class="style-cs-rolls" data-roll-type="Possession" data-stat="{{item.system.StatRolled.value}}"><img src="{{item.img}}"
								title="{{item.name}}" width="24" height="24" /></a>
					</div>
					<h4>{{item.name}}</h4>
				</div>
				<div class="item-statroll item-prop">{{item.system.StatRolled.value}}</div>
				<div class="item-controls">
					<a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
					<a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>
				</div>
			</li>
		{{/each}}
	{{/each}}
</ol>