<ol class="items-list">
	{{#each Possessions as |Possessions Category|}}
		<li class="item-list flexrow items-header">
			<div class="item-name" title="The Category of this Possession">{{Category}}</div>
			<div class="item-attacktype" title="The type of Attack of this Possession">Attack Type</div>
			<div class="item-statroll" title="The Stat Rolled to use this Possession">üé≤</div>
			<div class="item-actionslot" title="Which part of my turn does it take to use this Possession">‚è±</div>
			<div class="item-range" title="The Range of this Possession">üìè</div>
			{{#if ../isNarrator}}
				<div class="item-controls"><i class="fa-duotone fa-gears"></i></div>
			{{/if}}
		</li>
		{{#each Possessions as |item id|}}
			<li class="item flexrow" data-item-id="{{item._id}}">
				<div class="item-name flexrow">
					<div class="item-image">
						<img src="{{item.img}}" title="{{item.name}}" width="30" height="30" style="border: none" />
					</div>
					<h4>
						<div class="item-control item-edit style-view-properties" title="Possession Details">
							{{item.name}}
						</div>
					</h4>
				</div>
				<div class="item-attacktype item-prop">{{item.system.AttackType.value}}</div>
				<div class="item-statroll style-pos-rolls" data-roll-type="Possession"
					data-stat="{{item.system.Execution.StatRolled.value}}" data-item-name="{{item.name}}">
					{{item.system.Execution.StatRolled.value}}:
					{{lookup ../../system.RollStats item.system.Execution.StatRolled.value}}
				</div>
				<div class="item-actionslot item-prop">{{item.system.Execution.ActionSlot.value}}</div>
				<div class="item-range item-prop">{{item.system.Execution.Range.value}}</div>
				{{#if ../../isNarrator}}
					<div class="item-controls">
						<a class="item-control item-delete" title="Delete Possession"><i
								class="fa-duotone fa-trash"></i></a>
					</div>
				{{/if}}
			</li>
		{{/each}}
	{{/each}}
</ol>