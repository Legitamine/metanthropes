<div class="summary">
	<div class="flexrow">
		<div class="layout-container-small style-container-small">
			<div>Prime Metapower
				<input name="system.entermeta.primemetapower.value" type="text"
					value="{{system.entermeta.primemetapower.value}}" data-dtype="String"
					placeholder="Prime Metapower" />
			</div>
			<div>Metamorphosis
				<input name="system.entermeta.metamorphosis.value" type="text"
					value="{{system.entermeta.metamorphosis.value}}" data-dtype="String" placeholder="Metamorphosis" />
			</div>
			<div>Background
				<input name="system.Vital.background.value" type="text" value="{{system.Vital.background.value}}"
					data-dtype="String" placeholder="Background" />
			</div>
			<div>Arc
				<input name="system.Vital.arc.value" type="text" value="{{system.Vital.arc.value}}" data-dtype="String"
					placeholder="Arc" />
			</div>
			<div>Regression
				<input name="system.entermeta.regression.value" type="text"
					value="{{system.entermeta.regression.value}}" data-dtype="String" placeholder="Regression" />
			</div>
		</div>
		<div class="layout-container-small style-container-small">
			<div>Birthplace
				<input name="system.humanoids.birthplace.value" type="text"
					value="{{system.humanoids.birthplace.value}}" data-dtype="String" placeholder="City Name" />
			</div>
			<div>Age
				<input name="system.Vital.age.value" type="number" value="{{system.Vital.age.value}}"
					data-dtype="Number" placeholder="" />yr
			</div>
			<div>Gender
				<input name="system.humanoids.gender.value" type="text" value="{{system.humanoids.gender.value}}"
					data-dtype="String" placeholder="Gender" />
			</div>
			<div>Height
				<input name="system.humanoids.height.value" type="text" value="{{system.humanoids.height.value}}"
					data-dtype="String" placeholder="Height" />m
			</div>
			<div>Weight
				<input name="system.humanoids.weight.value" type="text" value="{{system.humanoids.weight.value}}"
					data-dtype="String" placeholder="Weight" />kg
			</div>
		</div>
		<div class="layout-container-small style-container-small">
			<div>Narrator
				<input name="system.entermeta.narrator.value" type="text" value="{{system.entermeta.narrator.value}}"
					data-dtype="String" placeholder="Narrator Name" />
			</div>
			<div>Saga
				<input name="system.entermeta.sagas.value" type="text" value="{{system.entermeta.sagas.value}}"
					data-dtype="String" placeholder="Saga Name" />
			</div>
			<div>Sessions Played
				<input name="system.entermeta.sessions.value" type="number" value="{{system.entermeta.sessions.value}}"
					data-dtype="Number" placeholder="0" />
			</div>
			<div>Coalition
				<input name="system.entermeta.coalition.value" type="text" value="{{system.entermeta.coalition.value}}"
					data-dtype="String" placeholder="Coalition Name" />
			</div>
			<div>Faction
				<input name="system.entermeta.faction.value" type="text" value="{{system.entermeta.faction.value}}"
					data-dtype="String" placeholder="Faction Name" />
			</div>
		</div>
	</div>
</div>